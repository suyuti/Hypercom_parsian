	<Config
		Name="Debug"
		Type="gnuc"
		DebugCallbackName="gdb"
		Version="1"
		OutputFile="%bdBaseApp.elf"
		CompilerConfigName="Latest Version"
		Defines='"/DMAKE_OPTIMUM" "/DDDL_MODULE" "/DC_DEV" "/DMAKE_VHDT" "/DMAKE_ENGLISH" "/DMAKE_GERMAN" "/DMAKE_SWEDISH" "/DMAKE_SPANISH" "/DMAKE_PORTUGUESE" "/DMAKE_FARSI" "/DDEFAULT_LANGUAGE=0" "/DSUPP_ADDICCTAGS" "/D_INCLUDE_NUCLEUS_FILE_" "/DMAKE_DEBUG" "/D_DEBUG" "/DDBGTRACING" "/DUSING_AAE" "/DAAE_MODEM" "/DVMOD=0x34" "/DVREV=0x31,0x32" "/DDYNAMIC_MEMORY_SIZE=%(DYNAMIC_MEMORY_SIZE)" "/DAPPLICATION_STACK_SIZE=%(APPLICATION_STACK_SIZE)" "/DMAKE_EMV" "/DVRELEASE=""FW500H_ """'>
		<Menu>
			<Target
				Name="Compile"
				MenuCaption="&amp;Compile"
				Dialog="_gnuc_options_form Compile"
				CaptureOutputWith="ProcessBuffer"
				Deletable="0"
				OutputExts="*.o"
				SaveOption="SaveCurrent"
				RunFromDir="%rw">
				<Exec
					CmdLine='%(CC) -c %xup %defd -g -fpic -o "%bd%n%oe" %i "%f" %~other'
					OtherOptions="%(CCFLAGS_DEBUG)"/>
			</Target>
			<Target
				Name="Link"
				MenuCaption="&amp;Link"
				ShowOnMenu="Never"
				Dialog="_gnuc_options_form Link"
				CaptureOutputWith="ProcessBuffer"
				Deletable="0"
				SaveOption="SaveCurrent"
				RunFromDir="%rw">
				<Exec
					CmdLine='%(LINK) %xup -o "%o" %objs %~other'
					OtherOptions='-shared --allow-multiple-definition --no-undefined --defsym HEAPSIZE=%(HEAPSIZE_DEBUG) -T %(DBG_LINKSCRIPT) -M -Map "%bd%on.map"'/>
			</Target>
			<Target
				Name="Build"
				MenuCaption="&amp;Build"
				Dialog="_gnuc_options_form Compile"
				CaptureOutputWith="ProcessBuffer"
				Deletable="0"
				SaveOption="SaveWorkspaceFiles"
				RunFromDir="%rw"
				Verbose="1"
				DependsRef="Debug">
				<Exec CmdLine='"%(VSLICKBIN1)vsbuild" "%w" "%r" -t build'/>
			</Target>
			<Target
				Name="Rebuild"
				MenuCaption="&amp;Rebuild"
				Dialog="_gnuc_options_form Compile"
				CaptureOutputWith="ProcessBuffer"
				Deletable="0"
				SaveOption="SaveWorkspaceFiles"
				RunFromDir="%rw"
				Verbose="1"
				DependsRef="Debug">
				<Exec CmdLine='"%(VSLICKBIN1)vsbuild" "%w" "%r" -t rebuild'/>
			</Target>
			<Target
				Name="Debug"
				MenuCaption="&amp;Debug"
				Dialog="_gnuc_options_form Run/Debug"
				BuildFirst="1"
				CaptureOutputWith="ProcessBuffer"
				Deletable="0"
				SaveOption="SaveNone"
				RunFromDir="%rw">
				<Exec CmdLine='vsdebugio -prog "%o"'/>
			</Target>
			<Target
				Name="Execute"
				MenuCaption="E&amp;xecute"
				Dialog="_gnuc_options_form Run/Debug"
				BuildFirst="1"
				CaptureOutputWith="ProcessBuffer"
				Deletable="0"
				SaveOption="SaveWorkspaceFiles"
				RunFromDir="%rw">
				<Exec CmdLine='"%o"'/>
			</Target>
			<Target
				Name="dash"
				MenuCaption="-"
				Deletable="0">
				<Exec/>
			</Target>
			<Target
				Name="GNU C Options"
				MenuCaption="GNU C &amp;Options..."
				ShowOnMenu="HideIfNoCmdLine"
				Deletable="0"
				SaveOption="SaveNone">
				<Exec
					CmdLine="gnucoptions"
					Type="Slick-C"/>
			</Target>
		</Menu>
