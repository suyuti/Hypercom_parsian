		</Libs>
		<PostBuildCommands>
			<Exec CmdLine='"%(GNU_TOOLS_BIN)\fixjmp" "%bd%on.elf" "%bd%on.rel"'/>
			<Exec CmdLine='%(OBJCOPY) --srec-len=255 -R .interp -R .hash -R .dynsym -R .dynstr -R .rel.init -R .rela.init -R .rel.text -R .rela.text -R .rel.fini -R .rela.fini -R .rel.rodata -R .rela.rodata -R .rel.data -R .rela.data -R .rel.tdata -R .rela.tdata -R .rel.tbss -R .rela.tbss -R .rel.ctors -R .rela.ctors -R .rel.dtors -R .rela.dtors -R .rel.got -R .rela.got -R .rel.sdata -R .rela.sdata -R .rel.sbss -R .rela.sbss -R .rel.sdata2 -R .rela.sdata2 -R .rel.sbss2 -R .rela.sbss2 -R .rel.bss -R .rela.bss -R .rel.plt -R .rela.plt -R .dynamic -O srec "%bd%on.elf" "%bd%on.src"'/>
			<Exec CmdLine='"%(GNU_TOOLS_BIN)\fixup" "%bd%on.src" "%bd%on.txt" "%bd%on.map" "%bd%on.rel" %(BINFORMAT)'/>
			<Exec CmdLine='"%(GNU_TOOLS_BIN)\rm" -f "%bd%on.rel"'/>
			<Exec CmdLine='"%(GNU_TOOLS_BIN)\DDLCompress" "%bd%on.txt" "%bd%on.app" %(BINFORMAT)'/>
			<Exec CmdLine='"%(GNU_TOOLS_BIN)\gawk" -f GenGDBScript.awk "%bd%on.elf" %(LOADADDR) &lt; "%bd%on.map" &gt; "%on.gdb"'/>
			<Exec CmdLine='echo ^&lt;Application Title="%(APPNAME)" Version="%(FREV)" Icon="%(FICON)" Company="Hypercom" DestinationDir="T4100" Main="%(FAPP).app" MainAttributes="0" UniqueName="%(APPNAME)" ^&gt; &gt; "%(FAPP)".xml'/>
			<Exec CmdLine="echo 	^&lt;Files^&gt; &gt;&gt; %(FAPP).xml"/>
			<Exec CmdLine='echo		 	^&lt;File URL="%b\%(FAPP).app"^/^&gt; &gt;&gt; %(FAPP).xml'/>
			<Exec CmdLine='echo		 	^&lt;File URL="%(FICON)"^/^&gt; &gt;&gt; %(FAPP).xml'/>
			<Exec CmdLine='echo		 	^&lt;File URL="debugflags"^/^&gt; &gt;&gt; %(FAPP).xml'/>
			<Exec CmdLine="echo 	^&lt;^/Files^&gt; &gt;&gt; %(FAPP).xml"/>
			<Exec CmdLine='echo	 	^&lt;StartUp CommandLine=""^/^&gt; &gt;&gt; %(FAPP).xml'/>
			<Exec CmdLine="echo ^&lt;^/Application^&gt; &gt;&gt; %(FAPP).xml"/>
			<Exec CmdLine='"%(PACKINGLISTBUILDER)" -g -x %(FAPP).xml -n %(FAPP)_%b.hxp'/>
			<Exec CmdLine='copy %(FAPP)_%b.hxp "%rn.hxp"'/>
			<Exec CmdLine='copy "%rn.hxp" "%bd"'/>
			<Exec CmdLine='"%(GNU_TOOLS_BIN)\rm" -f %(FAPP).xml'/>
		</PostBuildCommands>
	</Config>
