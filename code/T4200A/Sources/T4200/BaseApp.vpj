	<Config
		Name="SSL_Release"
		Type="gnuc"
		DebugCallbackName="gdb"
		Version="1"
		OutputFile="%bdBaseApp.elf"
		CompilerConfigName="Latest Version"
		Defines='"/DMAKE_SSL" "/DMAKE_OPTIMUM" "/DDDL_MODULE" "/DC_DEV" "/DMAKE_VHDT" "/DMAKE_ENGLISH" "/DMAKE_GERMAN" "/DMAKE_SWEDISH" "/DMAKE_SPANISH" "/DMAKE_PORTUGUESE" "/DMAKE_FARSI" "/DDEFAULT_LANGUAGE=0" "/DSUPP_ADDICCTAGS" "/D_INCLUDE_NUCLEUS_FILE_" "/DUSING_AAE" "/DAAE_MODEM" "/DVMOD=0x34" "/DVREV=0x31,0x32" "/DDYNAMIC_MEMORY_SIZE=%(DYNAMIC_MEMORY_SIZE)" "/DAPPLICATION_STACK_SIZE=%(APPLICATION_STACK_SIZE)" "/DMAKE_EMV" "/DVRELEASE=""FW500H_ """'>
		<Menu>
			<Target
				Name="Compile"
				MenuCaption="&amp;Compile"
				Dialog="_gnuc_options_form Compile"
