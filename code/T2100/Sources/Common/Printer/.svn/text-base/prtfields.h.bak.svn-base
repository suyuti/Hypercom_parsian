#define FD_DMY      11			// DATE - DDMMYY or MMYY
#define FD_YMD      12			// DATE - YYMMDD or YYMM
#define FD_MDYS     13			// DATE - MM/DD/YY or MM/YY
#define FD_DMYS     14			// DATE - DD/MM/YY or MM/YY
#define FD_YMDS     15			// DATE - YY/MM/DD or YY/MM
